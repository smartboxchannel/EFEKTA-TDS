        },
        {
          "endpoint": 1,
          "cluster": "0000_Basic",
          "attribute": "0236_Custom",
          "zclDataType": "SIGNED_8_BIT_INTEGER"
        },
		{
          "endpoint": 1,
          "cluster": "0000_Basic",
          "attribute": "0555_Custom",
          "zclDataType": "FLOAT_32_BIT"
        },
		{
          "endpoint": 1,
          "cluster": "0000_Basic",
          "attribute": "0556_Custom",
          "zclDataType": "FLOAT_32_BIT"
        },
		{
          "endpoint": 1,
          "cluster": "0000_Basic",
          "attribute": "0557_Custom",
          "zclDataType": "FLOAT_32_BIT"
        },
		{
          "endpoint": 1,
          "cluster": "0000_Basic",
          "attribute": "0558_Custom",
          "zclDataType": "FLOAT_32_BIT"
        },
		{
          "endpoint": 1,
          "cluster": "0402_TemperatureMeasurement",
          "attribute": "0210_Custom",
          "zclDataType": "SIGNED_16_BIT_INTEGER"
        },
		{
          "endpoint": 1,
          "cluster": "0402_TemperatureMeasurement",
          "attribute": "A19B_Custom",
          "zclDataType": "CHARACTER_STRING"
        }
      ]
    },
    {
      "link": {
        "endpoint": 1,
        "cluster": "0000_Basic",
        "attribute": "0555_Custom"
      },
      "bind": true,
      "report": {
        "min": 30,
        "max": 600,
        "change": 1
	  }
    },
	{
      "link": {
        "endpoint": 1,
        "cluster": "0402_TemperatureMeasurement",
        "attribute": "0000_MeasuredValue"
      },
      "bind": true,
      "report": {
        "min": 30,
        "max": 600,
        "change": 10
	  }
    }
  ],
  "services": [
    {
      "type": "AirQualitySensor",
	  "name": "Качество воды TDS",
      "logics": [
        {
          "type": "AirQualityFromPM2_5Density"
        }
      ],
      "characteristics": [
        {
          "type": "PM2_5Density",
          "link": [
            {
              "endpoint": 1,
              "cluster": "0000_Basic",
              "attribute": "0555_Custom"
            }
          ]
        }
      ]
    },
	{
      "type": "TemperatureSensor",
      "characteristics": [
        {
          "type": "CurrentTemperature",
          "link": {
            "endpoint": 1,
            "cluster": "0402_TemperatureMeasurement",
            "attribute": "0000_MeasuredValue"
          }
        }
      ]
    }
  ],
  "options": [
	{
        "link": {
          "endpoint": 1,
          "cluster": "0000_Basic",
          "attribute": "0201_Custom"
        },
        "name": "Интервал чтения сенсоров, в секундах",
        "type": "Integer",
		"unit": "сек.",
        "init": false,
        "value": 30,
        "minValue": 10,
        "maxValue": 3600,
        "minStep": 1
    },
	{
        "link": {
          "endpoint": 1,
          "cluster": "0402_TemperatureMeasurement",
          "attribute": "A19B_Custom"
        },
        "name": "Идентификатор сенсора DS18B20",
        "type": "String",
        "init": false,
        "write": false,
        "input": "status",
		"value": 0
    },
	{
      "link": {
        "endpoint": 1,
        "cluster": "0000_Basic",
        "attribute": "0557_Custom"
      },
      "name": "калибровка EC для сеносра TDS",
	  "description": "Коофициент калибровки значения EC",
	  "unit": "unit",
      "type": "Double",
      "init": false,
      "value": 0,
      "minValue": -10,
      "maxValue": 10,
      "minStep": 0.1
    },
	{
      "link": {
        "endpoint": 1,
        "cluster": "0000_Basic",
        "attribute": "0558_Custom"
      },
      "name": "калибровка Температурной компенсации для сеносра TDS",
	  "description": "Коофициент калибровки значения температуры",
	  "unit": "unit",
      "type": "Double",
      "init": false,
      "value": 0,
      "minValue": -10,
      "maxValue": 10,
      "minStep": 0.001
    },
	{
      "link": {
        "endpoint": 1,
        "cluster": "0000_Basic",
        "attribute": "0236_Custom"
      },
      "name": "Мощность радио передатчика",
	  "description": "Чем выше мощность, тем выше энергопотребление, на максимальной мощности в 19dbm, потребление 100mA при передаче, при можности 4dbm потребление 50mA",
      "type": "Integer",
      "init": false,
      "value": 19,
      "values": [
        {
          "value": 0,
          "name": "0dbm"
        },
        {
          "value": 4,
          "name": "4dbm"
        },
        {
          "value": 10,
          "name": "10dbm"
        },
        {
          "value": 19,
          "name": "19dbm"
        }
      ]
    },
	{
      "link": {
        "endpoint": 1,
        "cluster": "0000_Basic",
        "attribute": "0556_Custom"
      },
      "name": "Корректировка TDS",
	  "description": "Сдвиг значения TDS",
	  "unit": "ppm",
      "type": "Double",
	  "minStep": 0.1,
      "init": false,
      "value": 0,
      "minValue": -100.0,
      "maxValue": 100.0
    },
	{
      "link": {
        "endpoint": 1,
        "cluster": "0402_TemperatureMeasurement",
        "attribute": "0210_Custom"
      },
      "name": "Корректировка температуры",
	  "description": "Единица равна корректировке в 0.1°C.",
	  "unit": "°C",
      "type": "Integer",
      "init": false,
      "value": 1,
      "minValue": -100,
      "maxValue": 100
    }
  ]
}
